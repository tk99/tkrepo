<addon id="plugin.video.wwen"
       name="WWE Network"
       version="2.1.1"
       provider-name="bay_wolf">
  <requires>
    <import addon="xbmc.python" version="2.19.0"/>
    <import addon="script.module.requests" version="2.8.1"/>
    <import addon="script.module.m3u8" version="0.2.2"/>
    <import addon="script.module.dateutil" version="2.4.2"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="wwen_plugin.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Watch content from the WWE Network</summary>
    <description lang="en">Watch content from WWE, WCW and ECW through the WWE Network service.</description>
    <disclaimer lang="en">You need a valid WWE Network subscription for this addon to work.</disclaimer>
    <language>en</language>
    <platform>all</platform>
    <source>https://github.com/mjpbaywolf/plugin.video.wwen</source>
  </extension>
</addon>
