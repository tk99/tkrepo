<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.gamerszone"
	name="Gamers Zone"
	version="1.0.1"
	provider-name="Teknokat">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.addon.common" version="2.0.0"/>
    <import addon="plugin.video.youtube" version="5.0.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Gamers Zone</summary>
    <description lang="en">Youtube Channels from popular games consoles</description>
    <platform>all</platform>
    <forum></forum>
    <website></website>
  </extension>
</addon>
